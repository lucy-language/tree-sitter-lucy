{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lucy",
      "camelcase": "Lucy",
      "title": "Lucy",
      "scope": "source.lucy",
      "file-types": [
        "lc"
      ],
      "injection-regex": "^lucy$",
      "class-name": "TreeSitterLucy"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "The Lucy language",
    "authors": [
      {
        "name": "Kiana Bennett",
        "url": "https://github.com/chaoticva"
      }
    ],
    "links": {
      "repository": "https://github.com/lucy-language/tree-sitter-lucy"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}